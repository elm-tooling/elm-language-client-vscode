	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "build",
			"group": "build",
			"presentation": {
				"panel": "dedicated",
				"reveal": "never"
			},
			"problemMatcher": []
		},
		{
			"type": "npm",
			"script": "serve",
			"isBackground": true,
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"panel": "dedicated",
				"reveal": "never"
			},
			"problemMatcher": []
		},
		{
			"type": "npm",
			"script": "lint",
			"problemMatcher": [
				"$tslint5"
			]
		}
	]
}